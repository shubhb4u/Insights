            "sectionColumnGutterWidth" : "",
            "sectionConfig" : "{\"UUID\":\"3922b455-fb4d-4445-84c7-d008535b48cf\",\"columns\":[{\"UUID\":\"32f88071-ffbb-4f8c-87d2-a7b4d37d5a55\",\"columnName\":\"Column 1\",\"columnKey\":\"col1\",\"columnWidth\":\"12\",\"seedComponents\":[]}]}",
